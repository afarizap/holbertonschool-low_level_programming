#include <stdio.h>
#include "holberton.h"
/**
 * print_line - prints a number of lines if positive number.
 * @n: number of times line is printed
 */

void print_diagonal(int n)
{
	int l;
	int i;

	for (l = 0; l < n; l++)
	{
		for (i = 0; i <= l; i++)
		{
			_putchar(' ');
		}
		_putchar('\\');
		_putchar('\n');
	}
	_putchar('\n');
}

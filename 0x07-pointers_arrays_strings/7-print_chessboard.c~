#include "holberton.h"
#include <stdio.h>

/**
 * main - check the code for Holberton School students.
 *
 * Return: Always 0.
 */
void print_chessboard(char (*a)[8])
{
	int b;
	int c;

	for (c = 0; c < 8; c++)
		for (b = 0; b < 8; b++)
		{
			_putchar(a[b][c]);
			if (b == 7)
				_putchar('\n');
		}
}
